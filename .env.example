# Example environment variables file
# Copy this to .env and fill in your actual API keys

# Fact-checking APIs (optional)
FACTCHECK_API_KEY=your_key_here
GOOGLE_FACT_CHECK_API_KEY=your_key_here

# Social Media APIs (optional)
TWITTER_API_KEY=your_key_here
TWITTER_API_SECRET=your_key_here

# Database (optional - for production)
DATABASE_URL=sqlite:///./fake_detection.db

# Redis (optional - for caching)
REDIS_URL=redis://localhost:6379
